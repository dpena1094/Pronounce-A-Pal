<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sign_up_1"
    tools:context="edu.csun.team5.pronounce_a_pal.SignUp1Activity"
    tools:layout_editor_absoluteY="25dp">

    <ToggleButton
        android:id="@+id/Signup1MaleButton"
        android:layout_width="176dp"
        android:layout_height="53dp"
        android:layout_marginBottom="48dp"
        android:alpha="0.0
"
        android:text="ToggleButton"
        app:layout_constraintBottom_toTopOf="@+id/Signup1FemaleButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />

    <ToggleButton
        android:id="@+id/Signup1OtherButton"
        android:layout_width="182dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="206dp"
        android:alpha="0.0
"
        android:text="ToggleButton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />

    <ToggleButton
        android:id="@+id/Signup1FemaleButton"
        android:layout_width="176dp"
        android:layout_height="54dp"
        android:layout_marginBottom="60dp"
        android:alpha="0.0
"
        android:text="ToggleButton"
        app:layout_constraintBottom_toTopOf="@+id/Signup1OtherButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />
</android.support.constraint.ConstraintLayout>
